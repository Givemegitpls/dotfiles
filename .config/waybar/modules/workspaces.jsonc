{
  "group/workspaces-group": {
    "orientation": "horizontal",
    "modules": ["ext/workspaces"],
  },
  "ext/workspaces": {
    "format": "{icon}",
    "on-click": "activate",
    "on-scroll-down": "mmsg -d viewtoright,0",
    "on-scroll-up": "mmsg -d viewtoleft,0",
    "format-icons": {
      "active": "",
      "default": "",
      "urgent": "󰗖",
    },
  },
}
